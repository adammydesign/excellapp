RewriteEngine On
RewriteRule ^user/settings/([^/]*)$ /user/settings.php?user_id=$1 [L]
RewriteRule ^user/messages/([^/]*)$ /user/messages.php?user_id=$1 [L]
RewriteRule ^user/profile/([^/]*)$ /user/profile.php?user_id=$1 [L]
RewriteRule ^user/messages/([^/]*)/unread$ /user/messages.php?user_id=$1&subpage=unreadmessages [L]
RewriteRule ^user/availability/([^/]*)$ /user/availability.php?user_id=$1 [L]